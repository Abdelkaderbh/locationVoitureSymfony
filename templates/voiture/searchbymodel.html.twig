<h1> Search By Model </h1>

<form action="" method="POST">
Modele Name : <input type="text" name="modeleText">
    <input type="submit" value="Rechercher">

    {% if voiture is empty %}
    No Cars Found !
    {% else %}

        <table border="1">
            <tr>
                <td> Serie </td>
                <td> Modele </td>
            </tr>
            {% for v in voiture %}
                <tr>
                    <td>
                        {{ v.serie }}
                    </td>
                    <td>
                        {{ v.modele }}
                    </td>
                </tr>
            {% endfor %}
        </table>
    {% endif %}


</form>